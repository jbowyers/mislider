      "css": "./../css",
      "js": "./../js",
      "images": ""
    }
  },
  "fileNames": {
    "js": "mislider",
    "css": "mislider",
    "demo": "demo"
  },
  "homepage": "http://www.mislider.com",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-bower-installer": "~0.3.6",
