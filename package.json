    "build": "npm install && grunt",
    "start": "grunt watch",
    "test": "grunt"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jbowyers/mislider.git"
  },
  "keywords": [
    "slider",
    "jQuery",
    "plugin",
    "carousel",
    "image",
    "gallery",
    "multiple",
    "images"
  ],
  "licenses": [
    {
      "type": "GPL-3.0+",
      "url": "https://github.com/jbowyers/mislider/blob/master/licence.txt",
      "files": "LICENCE.txt"
    }
  ],
  "bugs": {
    "url": "https://github.com/jbowyers/mislider/issues"
  },
  "directories": {
