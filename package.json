      "images": "./demo/images"
    },
    "dist": {
      "root": "./dist",
      "js": "./dist/js",
      "lib": "./dist/lib",
      "css": "./dist/css",
      "images": ""
    },
    "doc": "./doc",
    "lib": "./lib",
    "templates": "./templates",
